<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tmvdl.nume" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Nume</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/android/icon.png" density="ldpi" />  <!-- 36x36 px -->
        <icon src="res/android/icon.png" density="mdpi" /> <!-- 48x48 px -->
        <icon src="res/android/icon.png" density="hdpi" /> <!-- 72x72 px -->
        <icon src="res/android/icon.png" density="xhdpi" /> <!-- 96x96 px -->
        <icon src="res/android/icon.png" density="xxhdpi" /> <!-- 144x144 px -->
        <icon src="res/android/icon.png" density="xxxhdpi" /> <!-- 192x192 px -->
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
